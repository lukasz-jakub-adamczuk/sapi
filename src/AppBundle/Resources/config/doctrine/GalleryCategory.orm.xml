<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\GalleryCategory" table="gallery_category">
    <id name="idGalleryCategory" type="smallint" column="id_gallery_category">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slug" type="string" column="slug" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
