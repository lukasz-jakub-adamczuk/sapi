<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\UserPermission" table="user_permission">
    <id name="idUserPermission" type="smallint" column="id_user_permission">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idUser" type="smallint" column="id_user" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="idPermission" type="boolean" column="id_permission" nullable="false"/>
  </entity>
</doctrine-mapping>
